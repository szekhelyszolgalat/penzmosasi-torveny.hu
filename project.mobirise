                "padding": "0 20px"
              },
              ".decor-wrapper": {
                "position": "absolute",
                "bottom": "0",
                "left": "-20rem",
                "height": "100%",
                "width": "100%",
                "display": "flex",
                "flex-direction": "column",
                "justify-content": "flex-end",
                "@media (max-width: 992px)": {
                  "display": "none"
                },
                ".decor": {
